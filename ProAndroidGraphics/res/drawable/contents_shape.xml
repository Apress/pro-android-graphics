<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
    <gradient
        android:startColor="#FF0000"
        android:centerColor="#FFFF00"
        android:endColor="#00FF00"
        android:centerX="0.5"
        android:centerY="0.5"
        android:gradientRadius="100"
        android:type="radial" />
